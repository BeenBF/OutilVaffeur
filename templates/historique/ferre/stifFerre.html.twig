<div class="container-fluid">
	<table id="myTable" class="table table-striped table-bordered" width="100%">
		<thead>
			<tr>
				<th>Produit</th>
				<th>Profil</th>
				<th>D&eacute;but val</th>
				<th>Fin Val</th>
				<th>Zones</th>
				<th>Horodatage</th>
				<th>Nature Valid</th>
				<th>Station</th>
				<th>Transporteur</th>
				<th>Ligne</th>
				<th>n&deg;Borne</th>
				<th>n&deg;Sequence</th>
			</tr>
		</thead>
		<tbody>
	{% for data in dataStif %}
		<tr>
			<td>{{ data.code_tarif }}</td>
			<td>{{ data.num_profil }}</td>
			<td>{{ data.date_debut_val|date('Y-m-d') }}</td>
			<td>{{ data.date_fin_val|date('Y-m-d') }}</td>
			<td>{{ data.zones }}</td>
			<td>{{ data.horodatage|date('Y-m-d H:i:s') }}</td>
			<td>{{ data.nature_transact }}</td>
			<td>{{ data.code_station_event }}</td>
			<td>{{ data.transporteur }}</td>
			<td>{{ data.ref_ligne }}</td>
			<td>{{ data.num_borne }}</td>
			<td>{{ data.sequence_ctu|number_format }}</td>
		</tr>
    {% endfor %}
		</tbody>
	</table>
	<a type="button" class="btn button" href="{{ path('recette') }}">Home.</a>
</div>